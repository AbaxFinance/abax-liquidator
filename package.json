{
  "name": "abax-liquidator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "yarn lint:format && yarn lint:fix",
    "lint:fix": "eslint . --ext .js,.ts,tsx --fix",
    "lint:format": "pretty-quick  --loglevel warn --write \"./**/*.{js,ts,tsx,md,json}\" ",
    "postinstall": "husky install",
    "generate-migration": "drizzle-kit generate:pg --out db/migrations --schema db/schema.ts",
    "migrate": "ts-node db/migrate",
    "generate:types": "yarn generate:defs && yarn generate:meta",
    "generate:defs": "yarn generate:defs:azerotestnet",
    "generate:defs:azerotestnet": "node --experimental-specifier-resolution=node --loader ts-node/esm  node_modules/.bin/polkadot-types-from-defs --package @azero-testnet/api-augment/azero-testnet/interfaces --input ./src/azero-testnet/interfaces --endpoint ./metadata-azero-testnet.json",
    "generate:meta": "yarn generate:meta:azerotestnet",
    "generate:meta:azerotestnet": "node --experimental-specifier-resolution=node --loader ts-node/esm node_modules/.bin/polkadot-types-from-chain --endpoint ./metadata-azero-testnet.json --package @azero-testnet/api-augment/azero-testnet/interfaces --output ./src/azero-testnet/interfaces"
  },
  "dependencies": {
    "@727-ventures/typechain-types": "^1.0.0-beta.1",
    "@abaxfinance/contract-helpers": "../abax-sdk/packages/contract-helpers",
    "@abaxfinance/utils": "../abax-sdk/packages/utils",
    "@polkadot/api": "10.9.1",
    "@polkadot/api-contract": "^10.3.2",
    "@polkadot/keyring": "^11.1.3",
    "@polkadot/typegen": "^10.10.1",
    "@polkadot/util": "^11.1.3",
    "@polkadot/util-crypto": "^11.1.3",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "ansi-to-html": "^0.7.2",
    "bn.js": "^5.2.1",
    "ccxt": "^3.0.75",
    "chalk": "^4.0.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "fs-extra": "^11.1.0",
    "json2csv": "^5.0.7",
    "large-set": "^1.0.3",
    "lint-staged": "^13.1.2",
    "lodash": "^4.17.21",
    "p-queue": "^6.6.2",
    "postgres": "^3.4.0",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.1",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.0.1",
    "@types/json2csv": "^5.0.3",
    "@types/lodash": "^4.14.191",
    "drizzle-kit": "^0.19.13",
    "husky": "^8.0.0",
    "tsconfig-paths": "^4.2.0"
  },
  "packageManager": "yarn@3.4.1"
}
